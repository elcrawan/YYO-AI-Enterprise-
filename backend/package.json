{
  "name": "yyo-ai-backend",
  "version": "1.0.0",
  "description": "YYO Agent AI - Enterprise Management System Backend",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback --all && npm run db:migrate && npm run db:seed",
    "docker:build": "docker build -t yyo-ai-backend .",
    "docker:run": "docker run -p 3000:3000 yyo-ai-backend"
  },
  "keywords": [
    "enterprise",
    "management",
    "ai",
    "nodejs",
    "typescript",
    "postgresql",
    "mongodb",
    "redis"
  ],
  "author": "YYO AI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "joi": "^17.11.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "multer": "^2.0.2",
    "sharp": "^0.32.6",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "@elastic/elasticsearch": "^8.11.0",
    "socket.io": "^4.7.4",
    "nodemailer": "^7.0.10",
    "twilio": "^4.19.0",
    "aws-sdk": "^2.1498.0",
    "openai": "^4.20.1",
    "@google/generative-ai": "^0.2.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.17.4",
    "prom-client": "^15.0.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "pdf-lib": "^1.17.1",
    "exceljs": "^4.4.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "node-cron": "^3.0.3",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.7",
    "@types/nodemailer": "^6.4.14",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
